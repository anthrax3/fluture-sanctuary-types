{
  "root": true,
  "extends": ["./node_modules/sanctuary-style/eslint-es3.json"],
  "parserOptions": {
    "sourceType": "script"
  },
  "env": {
    "browser": true
  },
  "globals": {
    "module": true,
    "require": true,
    "global": true
  },
  "rules": {
    "func-call-spacing": ["off"],
    "indent": ["off"],
    "no-unexpected-multiline": ["off"]
  }
}
